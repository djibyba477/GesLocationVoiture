<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="376.0" prefWidth="235.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.VoitureController">
   <children>
      <TableView fx:id="tabVoiture" layoutX="14.0" layoutY="102.0" onMouseClicked="#tabClick" prefHeight="227.0" prefWidth="207.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="69.0" text="ID" />
          <TableColumn fx:id="colNom" prefWidth="138.0" text="Nom" />
        </columns>
      </TableView>
      <Button fx:id="btnSupprimer" layoutX="82.0" layoutY="337.0" mnemonicParsing="false" onAction="#btnAction" prefHeight="25.0" prefWidth="72.0" text="Supprimer" />
      <TextField fx:id="txtNom" layoutX="72.0" layoutY="27.0" promptText="Nom voiture" />
      <Button fx:id="btnAjouter" layoutX="14.0" layoutY="61.0" mnemonicParsing="false" onAction="#btnAction" prefHeight="25.0" prefWidth="72.0" text="Ajouter" />
      <TextField fx:id="txtId" disable="true" layoutX="14.0" layoutY="27.0" prefHeight="25.0" prefWidth="46.0" promptText="ID" />
      <Button fx:id="btnAnnuler" layoutX="93.0" layoutY="61.0" mnemonicParsing="false" onAction="#btnAction" prefHeight="25.0" prefWidth="72.0" text="Annuler" />
   </children>
</AnchorPane>
